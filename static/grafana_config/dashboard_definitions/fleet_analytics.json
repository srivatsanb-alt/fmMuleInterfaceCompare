          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "\n\nSELECT\n  SUBSTRING(sherpa_name, 1, 6) AS Sherpa,\n  ROUND(SUM(EXTRACT(EPOCH FROM (end_time - start_time)))/((CASE WHEN $__timeFrom()::DATE = CURRENT_DATE THEN extract(epoch from current_timestamp::time) ELSE 86400 END/100)*COUNT(DISTINCT(start_time::DATE)))) AS Utilization\nFROM\n  trips\nWHERE\n  status = 'succeeded'\n  AND sherpa_name IS NOT NULL\n  AND start_time IS NOT NULL\n  AND end_time IS NOT NULL\n  AND DATE_TRUNC('day', start_time) = DATE_TRUNC('day', end_time)\n  AND start_time >= $__timeFrom() AND end_time <= $__timeTo()\n  AND fleet_name = '$fleetvar'\nGROUP BY\n  sherpa_name;",
          "refId": "A",
